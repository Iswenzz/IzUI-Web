{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"assets/*": [
				"src/assets/*",
				"../packages/izui-react/src/assets/*"
			],
			"styles/*": [
				"src/styles/*",
				"../packages/izui-react/src/styles/*"
			],
			"utils/*": [
				"src/utils/*",
				"../packages/izui-react/src/utils/*"
			],
			"test/*": [
				"src/__test__/*",
				"../packages/izui-react/src/__test__/*"
			],
			"pages": [
				"src/pages"
			],
			"components": [
				"src/components",
				"../packages/izui-react/src/components"
			]
		}
	}
}
