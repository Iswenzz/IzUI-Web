{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/test/*": ["src/__test__/*"],
			"@/components": ["src/components"],
			"@/config/*": ["src/config/*"],
			"@/router/*": ["src/router/*"],
			"@/styles/*": ["src/styles/*"],
			"@/utils/*": ["src/utils/*"],

			"@/fixtures/test/*": ["fixtures/__test__/*"],
			"@/fixtures/assets/*": ["fixtures/assets/*"],
			"@/fixtures/components": ["fixtures/components"],
			"@/fixtures/config/*": ["fixtures/config/*"],
			"@/fixtures/pages": ["fixtures/pages"],
			"@/fixtures/router/*": ["fixtures/router/*"],
			"@/fixtures/styles/*": ["fixtures/styles/*"],
			"@/fixtures/utils/*": ["fixtures/utils/*"]
		}
	}
}
