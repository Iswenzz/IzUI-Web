{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"declaration": true,
		"declarationDir": "build",
		"outDir": "build",
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "ESNext",
		"moduleResolution": "node",
		"resolveJsonModule": true
	},
	"include": ["commands", "mock", "utils"]
}
